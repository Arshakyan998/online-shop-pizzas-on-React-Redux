enu__popUp\" ref={Ref}>\r\n        сортировать по\r\n        <span className=\"active\">{sortType}</span>\r\n        <div className={visible ? \"\" : \"hidden\"}>\r\n          {sortBy.map((element, i) => {\r\n            return (\r\n              <li\r\n                key={i}\r\n                onClick={() => changePopUp(element)}\r\n                className={activeSortItem ===element.type  ? \"active\" : \"\"}\r\n              >\r\n                {element.name}\r\n              </li>\r\n            );\r\n          })}\r\n        </div>\r\n      </ul>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default Menu;\r\n","C:\\Users\\User\\Desktop\\react\\online-shop-phone\\src\\components\\main\\pizzaBlock\\GetPizzasItem.jsx",[],"C:\\Users\\User\\Desktop\\react\\online-shop-phone\\src\\components\\main\\pizzaBlock\\Pizzas.jsx",["80"],"import React from 'react'\r\nimport { useSelector,useDispatch } from 'react-redux'\r\n\r\nimport RenderPizzas from './RenderPizzas'\r\nimport \"./pizzas.scss\"\r\nimport MyLoader from './LodingBlock'\r\nimport { fetchPizzas } from '../../../redux/actions/pizzas'\r\nimport {addPizzaCart} from '../../../redux/actions/cart'\r\n\r\n\r\nfunction PizzaItem() {\r\n        const dispatch=useDispatch()\r\n\r\n        const {itemsPizzas,cart} = useSelector(state => {\r\n                return{\r\n                        itemsPizzas:state.setPizzas.items,\r\n                        cart:state.cart.items,\r\n                }\r\n        })\r\n\r\n\r\n        const {isLoaded} = useSelector(state => {\r\n                return{\r\n                        isLoaded:state.setPizzas.isLoaded\r\n                }\r\n        })\r\n\r\n        const {catygory,sortBy,order} = useSelector(state => {\r\n                return{\r\n                        catygory:state.sortBy.catygory,\r\n                        sortBy:state.sortBy.sortBy.type,\r\n                        order:state.sortBy.sortBy.order\r\n                }\r\n        })\r\n\r\n        const addPizzaToCart=(obj)=>{\r\n\r\n               dispatch(addPizzaCart(obj))\r\n\r\n        }\r\n\r\n        React.useEffect(()=>{\r\ndispatch(fetchPizzas(catygory,sortBy,order))\r\n        },[catygory,sortBy,order])\r\n\r\n\r\n        return (\r\n                <div className='controll_pizzas_block'>\r\n                        {\r\n                              isLoaded ?itemsPizzas.map((element,i)=>{\r\n                                       return  <RenderPizzas\r\n                                        key={element.id}\r\n                                        {...element}\r\n                                        addPizzaToCart={addPizzaToCart}\r\n                                        cart={cart}\r\n                                       />\r\n                                }):new Array(12).fill(0).map((_,i)=><MyLoader key={i}/>)\r\n                        }\r\n                        \r\n                        \r\n                </div>\r\n        )\r\n}\r\n\r\nexport default PizzaItem\r\n","C:\\Users\\User\\Desktop\\react\\online-shop-phone\\src\\components\\main\\pizzaBlock\\RenderPizzas.jsx",[],"C:\\Users\\User\\Desktop\\react\\online-shop-phone\\src\\redux\\root.js",[],"C:\\Users\\User\\Desktop\\react\\online-shop-phone\\src\\redux\\rducers\\filter.js",[],"C:\\Users\\User\\Desktop\\react\\online-shop-phone\\src\\redux\\actions\\filter.js",[],"C:\\Users\\User\\Desktop\\react\\online-shop-phone\\src\\Other.jsx",[],"C:\\Users\\User\\Desktop\\react\\online-shop-phone\\src\\redux\\actions\\pizzas.js",[],"C:\\Users\\User\\Desktop\\react\\online-shop-phone\\src\\redux\\rducers\\pizzas.js",[],"C:\\Users\\User\\Desktop\\react\\online-shop-phone\\src\\redux\\actions\\fetchApi.js",[],"C:\\Users\\User\\Desktop\\react\\online-shop-phone\\src\\components\\main\\cart\\Cart.jsx",[],"C:\\Users\\User\\Desktop\\react\\online-shop-phone\\src\\components\\main\\pizzaBlock\\LodingBlock.jsx",[],"C:\\Users\\User\\Desktop\\react\\online-shop-phone\\src\\redux\\rducers\\cart.js",[],"C:\